"2"
(define (quad-poly a b c)
  (let ((sqrt (expt (- (expt b 2) (* 4 a c)) .5)))
  (cond 
    ((< sqrt 0) 'no-reals)
    ((= sqrt 0) 1)
    ((> sqrt 0) 2))))
 
(quad-poly 1 2 1)
(quad-poly 3 0 1)
(quad-poly 2 3 1)



